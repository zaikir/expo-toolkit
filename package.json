{
  "name": "@kirz/expo-toolkit",
  "version": "0.4.26",
  "files": [
    "lib",
    "appsflyer",
    "branch",
    "facebook",
    "idfa",
    "onesignal",
    "sentry",
    "user-identity",
    "idfv",
    "apphud",
    "localization"
  ],
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/typescript/index.d.ts",
      "import": "./lib/module/index.js",
      "default": "./lib/commonjs/index.js"
    },
    "./appsflyer": {
      "types": "./lib/typescript/plugins/appsflyer/index.d.ts",
      "import": "./lib/module/plugins/appsflyer/index.js",
      "default": "./lib/commonjs/plugins/appsflyer/index.js"
    },
    "./sentry": {
      "types": "./lib/typescript/plugins/sentry/index.d.ts",
      "import": "./lib/module/plugins/sentry/index.js",
      "default": "./lib/commonjs/plugins/sentry/index.js"
    },
    "./branch": {
      "types": "./lib/typescript/plugins/branch/index.d.ts",
      "import": "./lib/module/plugins/branch/index.js",
      "default": "./lib/commonjs/plugins/branch/index.js"
    },
    "./facebook": {
      "types": "./lib/typescript/plugins/facebook/index.d.ts",
      "import": "./lib/module/plugins/facebook/index.js",
      "default": "./lib/commonjs/plugins/facebook/index.js"
    },
    "./idfa": {
      "types": "./lib/typescript/plugins/idfa/index.d.ts",
      "import": "./lib/module/plugins/idfa/index.js",
      "default": "./lib/commonjs/plugins/idfa/index.js"
    },
    "./onesignal": {
      "types": "./lib/typescript/plugins/onesignal/index.d.ts",
      "import": "./lib/module/plugins/onesignal/index.js",
      "default": "./lib/commonjs/plugins/onesignal/index.js"
    },
    "./user-identity": {
      "types": "./lib/typescript/plugins/user-identity/index.d.ts",
      "import": "./lib/module/plugins/user-identity/index.js",
      "default": "./lib/commonjs/plugins/user-identity/index.js"
    },
    "./idfv": {
      "types": "./lib/typescript/plugins/idfv/index.d.ts",
      "import": "./lib/module/plugins/idfv/index.js",
      "default": "./lib/commonjs/plugins/idfv/index.js"
    },
    "./apphud": {
      "types": "./lib/typescript/plugins/apphud/index.d.ts",
      "import": "./lib/module/plugins/apphud/index.js",
      "default": "./lib/commonjs/plugins/apphud/index.js"
    },
    "./localization": {
      "types": "./lib/typescript/plugins/localization/index.d.ts",
      "import": "./lib/module/plugins/localization/index.js",
      "default": "./lib/commonjs/plugins/localization/index.js"
    }
  },
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build && npx tsc-alias -p tsconfig.main.json && npx tsc-alias -p tsconfig.module.json && npx tsc-alias -p tsconfig.typescript.json",
    "release": "npm run prepare && npm publish --access public"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/runtime": "^7.12.5",
    "@kirz/expo-apphud": "^0.1.1",
    "@sentry/react-native": "^5.33.1",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.2",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-universe": "^11.1.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-exports": "^1.0.0-beta.4",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "expo": "^49.0.7",
    "expo-application": "^5.9.1",
    "expo-router": "^3.5.23",
    "expo-tracking-transparency": "^4.0.2",
    "expo-user-identity": "^0.4.0",
    "jotai": "^2.10.0",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-native": "0.72.3",
    "react-native-appsflyer": "^6.15.1",
    "react-native-branch": "^6.3.0",
    "react-native-builder-bob": "^0.20.3",
    "react-native-fbsdk-next": "^13.1.3",
    "react-native-mmkv": "^2.12.2",
    "react-native-onesignal": "^5.2.5",
    "react-native-reanimated": "^3.15.4",
    "tsc-alias": "^1.8.2",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "jotai": "^2.0.0",
    "react": "^18.0.0",
    "react-native": ">=0.70.0 <0.80.0",
    "react-native-mmkv": "^2.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "dependencies": {
    "@formatjs/intl": "^2.10.8",
    "chalk": "^4.1.2",
    "expo-localization": "^15.0.3",
    "intl": "^1.2.5",
    "react-native-error-boundary": "^1.2.5"
  }
}
