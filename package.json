    "react-native": "0.72.3",
    "react-native-appsflyer": "^6.15.1",
    "react-native-branch": "^6.3.0",
    "react-native-builder-bob": "^0.20.3",
    "react-native-fbsdk-next": "^13.1.3",
    "react-native-mmkv": "^2.12.2",
    "react-native-onesignal": "^5.2.5",
    "react-native-reanimated": "^3.15.4",
    "react-native-smartlook-analytics": "^2.1.19",
    "tsc-alias": "^1.8.2",
    "typescript": "^5.1.6",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "jotai": "^2.0.0",
    "react": "^18.0.0",
    "react-native": ">=0.70.0 <0.80.0",
    "react-native-mmkv": "^2.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "inquirer": "^12.0.1",
    "ora": "^8.1.1",
    "react-native-error-boundary": "^1.2.5",
    "recast": "^0.23.9",
    "ts-morph": "^24.0.0"
  }
}